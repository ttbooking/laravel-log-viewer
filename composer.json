{
    "name": "ttbooking/laravel-log-viewer",
    "description": "A Laravel log reader",
    "license": "MIT",
    "keywords": [
        "log",
        "log-reader",
        "log-viewer",
        "logging",
        "laravel",
        "lumen"
    ],
    "type": "laravel-package",
    "authors": [
        {
            "name": "rap2hpoutre",
            "email": "raphaelht@gmail.com"
        },
        {
            "name": "Sergey Danilchenko",
            "email": "s.danilchenko@ttbooking.ru"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3|^10.0|^11.0|^12.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "classmap": [
            "src/controllers"
        ],
        "psr-0": {
            "Rap2hpoutre\\LaravelLogViewer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rap2hpoutre\\LaravelLogViewer\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rap2hpoutre\\LaravelLogViewer\\LaravelLogViewerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
